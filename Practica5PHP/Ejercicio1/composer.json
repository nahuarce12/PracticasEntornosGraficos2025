{
    "name": "practica5php/email-sender",
    "description": "PHP Email Sender with HTML Support using PHPMailer",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "phpmailer/phpmailer": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true
    }
}
